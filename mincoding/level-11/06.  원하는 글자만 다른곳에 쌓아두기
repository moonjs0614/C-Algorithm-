#include <iostream>
using namespace std;

char arr[8];

int main(){
  
  for (int i = 0; i < 8; i++)
  {
    cin >> arr[i];
  }
  
  char big[8] = {'!'};
  char small[8] = {'!'};
  
  for (int i = 0; i < 8; i++)
  {
    if (arr[i] >= 'A' and arr[i] <= 'Z')
    {
      big[i] = arr[i];
    }
    else
    {
      small[i] = arr[i];
    }
  }
  
  cout << "big=";
  for (int i = 0; i < 8; i++)
  {
    if (big[i] >= 'A' and big[i] <= 'Z')
    {
      cout << big[i];
    }
  }
  cout << endl;
  
  cout << "small=";
  for (int i = 0; i < 8; i++)
  {
    if (small[i] >= 'a' and small[i] <= 'z')
    {
      cout << small[i];
    }
  }
  
  return 0;}
