#include <iostream>
using namespace std;

char s1[10];
char s2[10];

int main(){
  
  cin >> s1;
  cin >> s2;
  
  int c1 = 0;
  int c2 = 0;
  
  for (int i = 0; i < 10; i++)
  {
    if (s1[i] != '\0')
    {
      c1++;
    }
    
    if (s2[i] != '\0')
    {
      c2++;
    }
  }
  
  cout << c1 << " " << c2;
  
  return 0;}
