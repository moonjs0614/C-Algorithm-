#include <iostream>
using namespace std;

int arr[5] = {3, 5, 2, 4, 1};
int vect[3][2] = {{9, 8}, {7, 1}, {3, 4}};

int main(){
  
  int a;
  cin >> a;
  
  if (a % 2 == 1)
  {
    for (int i = 0; i < 5; i++)
    {
      cout << arr[i];
    }
  }
   else
   {
     for (int i = 0; i < 3; i++)
     {
       for (int j = 0; j < 2; j++)
       {
         cout << vect[i][j];
       }
       cout << endl;
     }
   }
   return 0;}
