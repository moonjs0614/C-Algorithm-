//KFC, MC함수를 정의하세요
//KFC함수 : "#"을 10회 출력
//MC함수 : "@"을 10회 출력
//main함수에서 두 함수를 각각 한번씩 호출하세요

#include <iostream>
using namespace std;

void KFC()
{
  for (int i = 0; i < 10; i++)
  {
    cout << "#";
  }
}

void MC()
{
  for (int i = 0; i < 10; i++)
  {
    cout << "@";
  }
}

int main(){
  
  KFC();
  cout << endl;
  MC();
  
  return 0;}
