#include <iostream>
using namespace std;

int arr[3][4] = {{4, 3, 1, 1}, {3, 1, 2, 1}, {0, 0, 1, 2}};

int main(){
  
  int a;
  cin >> a;
  int count = 0;
  
  for (int i = 0; i < 3; i++)
  {
    for (int j = 0; j < 4; j++)
    {
      if (arr[i][j] == a)
        count ++;
    }
  }
  
  cout << count << "개 존재합니다";
  
  return 0;}
