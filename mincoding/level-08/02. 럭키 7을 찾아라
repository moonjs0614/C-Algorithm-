//숫자 6개를 배열에 입력 받으세요.
//그리고 for문을 이용하여 0번 index 부터 5번 index까지 배열 안에 있는 값을 하나씩 출력합니다.
//그러다가 만약 출력할 값이 7이 나온다면 중단(break)를 시킵니다.

#include <iostream>
using namespace std;

int main(){
  
  int arr[6];
  for (int i = 0; i < 6; i++)
  {
    cin >> arr[i];
  }
  
  for (int i = 0; i < 6; i++)
  {
    if (arr[i] != 7)
    {
      cout << arr[i] << " ";
    }
    else
    {
      break;
    }
  }
  
  return 0;}
